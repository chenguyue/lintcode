public class Solution {
    /**
     * @param s : A string
     * @return : A string
     */
    public String reverseWords(String s) {
        // write your code
        String str[] = s.split(" ");
        String s1 = "";
        for(int i = str.length - 1;i >= 0;i--)
        {
            s1 = s1 + str[i]+" ";
        }
        s1 = s1.trim();
        return s1;
    }
}
